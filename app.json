{
  "expo": {
    "name": "kaizen karta app",
    "slug": "kaizen",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "android": {
      "package": "com.opentiq.kaizen"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.opentiq.kaizenkarta",
      "infoPlist": {
   "NSCameraUsageDescription": "This app uses the camera to change profile photo.",
   "NSPhotoLibraryUsageDescription":"This app uses the photo library to change profile photo.",
 }
    }
  }
}
